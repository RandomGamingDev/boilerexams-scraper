        "resources": [
          {
            "id": "65e925d3-48d3-4570-afeb-49385137623f",
            "type": "CODE",
            "data": {
              "content": "for n in range(1, 33550337):\n    sum = 0\n    i = 1\n    while i < n:\n        sum += i if n % i == 0 else 0\n        i += 1\n    if sum == 1:\n        print(n)\n",
